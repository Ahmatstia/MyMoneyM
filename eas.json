{
  "cli": {
    "version": ">= 4.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "env": {
        "EXPO_NO_NEW_ARCH": "true"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EXPO_NO_NEW_ARCH": "true",
        "EXPO_PUBLIC_ENABLE_NOTIFICATIONS": "true"
      },
      "channel": "preview",
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      },
      "env": {
        "EXPO_NO_NEW_ARCH": "true",
        "EXPO_PUBLIC_ENABLE_NOTIFICATIONS": "true"
      },
      "channel": "production",
      "distribution": "store",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
