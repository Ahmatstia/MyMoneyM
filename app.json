{
  "expo": {
    "name": "MyMoney",
    "slug": "MyMoney",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/lg.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/lg1.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/lg.png",
        "backgroundColor": "#0F172A"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/lg.png"
    },
    "plugins": ["expo-font"]
  }
}
